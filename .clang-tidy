#WarningsAsErrors: '*'
HeaderFilterRegex: '*.(h|hpp|hxx)'
AnalyzeTemporaryDtors: false
CheckOptions:
  - key:             readability-identifier-naming.AbstractClassCase
    value:           CamelCase
  - key:             readability-identifier-naming.ClassCase
    value:           CamelCase
